{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": [true, "no-public"],
    "ordered-imports": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "prefer-const": false,
    "no-empty": false,
    "no-namespace": false,
    "no-reference": false
  }
}
